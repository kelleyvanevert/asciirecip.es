{
  "env": {},
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.+)",
      "dest": "public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
